{
  "compilerOptions": {
    "outDir": "dist",
    "rootDir": "src",
    "lib": [
      "esnext"
    ],
    "strict": true,
    "noImplicitAny": false,
    "target": "es6",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "strictNullChecks": true,
    "resolveJsonModule": true,
    "exactOptionalPropertyTypes": false
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}